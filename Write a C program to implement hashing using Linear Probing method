#include <stdio.h>
#define SIZE 10

int main() {
    int hash[SIZE], keys[] = {12, 25, 35, 26, 29}, n = 5;
    int i, index;

    // Initialize hash table
    for(i = 0; i < SIZE; i++)
        hash[i] = -1;

    // Insert keys
    for(i = 0; i < n; i++) {
        index = keys[i] % SIZE;
        while(hash[index] != -1)  // Linear probing
            index = (index + 1) % SIZE;
        hash[index] = keys[i];
    }

    // Display table
    printf("Index\tValue\n");
    for(i = 0; i < SIZE; i++)
        printf("%d\t%d\n", i, hash[i]);

    return 0;
}
