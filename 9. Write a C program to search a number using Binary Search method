#include <stdio.h>

int main() {
    int a[100], n, x, low, high, mid, found = 0;

    printf("Enter number of elements: ");
    scanf("%d", &n);

    printf("Enter %d sorted elements:\n", n);
    for (int i = 0; i < n; i++)
        scanf("%d", &a[i]);

    printf("Enter number to search: ");
    scanf("%d", &x);

    low = 0;
    high = n - 1;

    while (low <= high) {
        mid = (low + high) / 2;
        if (a[mid] == x) {
            printf("Found at position %d\n", mid);
            found = 1;
            break;
        } else if (a[mid] < x)
            low = mid + 1;
        else
            high = mid - 1;
    }

    if (!found)
        printf("Not found.\n");

    return 0;
}
